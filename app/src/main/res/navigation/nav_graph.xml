    </fragment>
    <fragment
        android:id="@+id/bookingFragment"
        android:name="com.example.hanginapp.BookingsFragment"
        android:label="fragment_bookings"
        tools:layout="@layout/fragment_bookings"/>
    <fragment
        android:id="@+id/nearbyFragment"
        android:name="com.example.hanginapp.NearbyFragment"
        android:label="fragment_nearby"
        tools:layout="@layout/fragment_nearby"/>
    <fragment
        android:id="@+id/more_Fragment"
        android:name="com.example.hanginapp.MoreFragment"
        android:label="fragment_more"
        tools:layout="@layout/fragment_more"/>
    <fragment
    android:id="@+id/place_details_fragment"
    android:name="com.example.hanginapp.PlaceDetailsFragment"
    android:label="fragment_placeDetails"
