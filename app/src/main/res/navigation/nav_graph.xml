        android:id="@+id/homeFragment"
        android:name="com.example.hanginapp.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_PlaceDetailsFragment"
